<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.musicgallery.mapper.MusicMapper">

    <resultMap id="BaseResultMap" type="com.example.musicgallery.model.Music">
        <id column="id" property="id" />
        <result column="title" property="title" />
        <result column="genre" property="genre" />
        <result column="duration" property="duration" />
        <result column="artist_id" property="artistId" />
        <result column="album_id" property="albumId" />
    </resultMap>

    <sql id="Base_Column_List">
        id, title, genre, duration, artist_id, album_id
    </sql>

</mapper>
